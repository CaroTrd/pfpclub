{
  "name": "pfpclub",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "babel-eslint": "^10.0.1",
    "google-map-react": "^1.1.2",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-redux": "^6.0.0",
    "react-router": "^3.2.1",
    "react-router-dom": "^4.3.1",
    "react-router-redux": "^4.0.8",
    "react-scripts": "2.1.3",
    "redux": "^4.0.1",
    "redux-form": "^8.1.0",
    "redux-thunk": "^2.3.0",
    "sass-loader": "^7.1.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint-all": "eslint src",
    "precommit": "npm run lint-all"
  },
  "proxy": "http://localhost:4000/",
  "eslintConfig": {
    "extends": "airbnb",
    "parser": "babel-eslint",
    "env": {
      "es6": true,
      "browser": true
    },
    "parserOptions": {
      "sourceType": "module"
    },
    "rules": {
      "react/jsx-filename-extension": [
        "js",
        "jsx"
      ],
      "jsx-a11y/href-no-hash": "off",
      "jsx-a11y/mouse-events-have-key-events": "off",
      "react/prop-types": "off",
      "react/destructuring-assignment": "off",
      "jsx-a11y/label-has-associated-control": [
        2,
        {
          "labelComponents": [
            "CustomLabel"
          ],
          "labelAttributes": [
            "inputLabel"
          ],
          "controlComponents": [
            "CustomInput"
          ],
          "assert": "both",
          "depth": 3
        }
      ]
    }
  },
  "devDependencies": {
    "eslint-config-airbnb": "^17.1.0",
    "husky": "^1.3.1"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
